<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="SecureFolderFS.AvaloniaUI.App"
             xmlns:sty="using:FluentAvalonia.Styling">
    <Application.Styles>
        <sty:FluentAvaloniaTheme PreferUserAccentColor="True" />
        <StyleInclude Source="/Styles/ControlFixStyles.axaml" />
    </Application.Styles>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/ResourceDictionaries/ControlConsistencyResources.axaml" />
				<ResourceInclude Source="/ResourceDictionaries/ImageResources.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
