<ContentView
    x:Class="SecureFolderFS.Maui.Views.Wizard.SummaryWizardViewControl"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SecureFolderFS.Maui.Views.Wizard"
    x:DataType="local:SummaryWizardViewControl">

    <VerticalStackLayout HorizontalOptions="Center">
        <Label
            HorizontalOptions="Center"
            Text="Vault has been added!"
            VerticalOptions="Center" />
        <Label Text="{Binding ViewModel.VaultName, Mode=OneWay}" />
    </VerticalStackLayout>
</ContentView>
