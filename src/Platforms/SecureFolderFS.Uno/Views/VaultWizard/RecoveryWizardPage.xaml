<Page
    x:Class="SecureFolderFS.Uno.Views.VaultWizard.RecoveryWizardPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uc="using:SecureFolderFS.Uno.UserControls"
    mc:Ignorable="d">

    <uc:RecoveryPreviewControl
        ExportCommand="{x:Bind ViewModel.RecoveryViewModel.ExportCommand}"
        IsExpanded="True"
        MasterKey="{x:Bind ViewModel.RecoveryViewModel.MasterKey, Mode=OneWay}" />
</Page>
